@page "/admin/members"
@using ZauberCMS.Core
@layout SectionWithTree

<PageTitle>Members</PageTitle>

<SectionDashboards SectionAlias="@(Constants.Sections.MembersSection)"/>

@code {
    [CascadingParameter] protected SectionWithTree? Layout { get; set; }

    protected override void OnInitialized()
    {
        base.OnInitialized();
        Layout?.SetSection(Constants.Sections.MembersSection);
    }
}